<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="modelo.Horario" table="horarios" >
    <id name="id" column="horario_id">
        <generator class="identity" />
    </id>
    
    <property name="entrada" column="fecha_entrada" />
    <property name="salida" column="fecha_salida" />

       
  </class>
</hibernate-mapping>
