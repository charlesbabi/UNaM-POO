<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="modelo.Horario" table="horarios" >
    <id name="id" column="horario_id">
        <generator class="identity" />
    </id>
    <property name="horarioEntrada" column="horarioEntrada" />
    <property name="diaEntrada" column="diaEntrada" />
    <property name="horarioSalida" column="horarioSalida" />
    <property name="diaSalida" column="diaSalida" />   
    
       
  </class>
</hibernate-mapping>
